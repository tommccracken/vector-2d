{
  "name": "vector-2d",
  "version": "0.1.0",
  "description": "A 2D vector math module written in TypeScript",
  "main": "./dist/mod.js",
  "scripts": {
    "build": "npm run-script build-module && npm run-script test && npm run-script build-docs",
    "build-module": "tsc",
    "build-docs": "typedoc --mode file ./src/mod.ts",
    "test": "nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tommccracken/vector-2d.git"
  },
  "keywords": [
    "vector",
    "vector 2D",
    "math",
    "physics",
    "mathematics"
  ],
  "homepage": "https://github.com/tommccracken/vector-2d",
  "author": "Thomas O. McCracken",
  "license": "MIT",
  "devDependencies": {
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "typedoc": "^0.17.0",
    "typescript": "^4.1.3"
  }
}
